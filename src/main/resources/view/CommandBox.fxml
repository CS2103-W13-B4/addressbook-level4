<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="commandPane" styleClass="anchor-pane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <TextField fx:id="commandTextFieldKeyword" editable="false" maxWidth="-Infinity" StackPane.alignment="CENTER_LEFT">
      <padding>
         <Insets left="4.0" />
      </padding>
      <StackPane.margin>
         <Insets left="4.5" />
      </StackPane.margin></TextField>
  <TextField fx:id="commandTextField" onAction="#handleCommandInputChanged" onKeyPressed="#handleKeyPress" onKeyReleased="#handleKeyReleased" promptText="Enter command here..." StackPane.alignment="CENTER_LEFT" />
</StackPane>
